    "license": "BSD-2-Clause",
    "require": {
        "php": ">=5.4.0",
        "zendframework/zendframework": "~2.3",
        "matryoshka-model/matryoshka": "0.7.*@dev",
        "matryoshka-model/service-api": "0.4.*@dev",
